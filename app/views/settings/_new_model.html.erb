<%= error_messages_for(@setting) %>
<div class="col-sm-6">
  <table summary="New Model" role="form">
    <%= f.hidden_field(:setting_name, :value => "model_description") %>
    <div class="form-group">
      <%= f.label(:setting_model, "Model Name", label: "Model Name") %>
      <%= f.text_field(:setting_model, class: "form-control", placeholder: "Model name. eg: incidents - No Spaces") %>
    </div>
    <div class="form-group">
      <%= f.label(:setting_nice, "Model Name", label: "Model Name") %>
      <%= f.text_field(:setting_nice, class: "form-control", placeholder: "The Nice name for your Model") %>
    </div>
    <div class="form-group">
      <%= f.label(:setting_value, "Description", label: "Description") %>
      <%= f.text_area(:setting_value, :rows => 5, type: "text", class: "form-control", placeholder: "Short description of the model") %>
    </div>
</div>

      
